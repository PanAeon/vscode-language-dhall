{
    "name": "dhall-lang",
    "displayName": "dhall lang",
    "description": "Syntax highlighting for the dhall programming language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dhall",
                "aliases": [
                    "Dhall",
                    "dhall"
                ],
                "extensions": [
                    "dhall"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dhall",
                "scopeName": "source.dhall",
                "path": "./syntaxes/dhall.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.12.1"
    }
}
